@{
  Layout="Shared/_Layout.cshtml";
}

<h2>Adding Artist to @Model["rack"].GetTitle()</h2>


<form class="form-control" action='/cd/@Model["rack"].GetId()/artists' method="post">
  <input id='rack-id' name='rack-id' type='hidden' value='@Model["rack"].GetId()'>
  <label for="artist_name">Artist Name</label>
  <input id="artist_name" type="text" name="artist_name">
</form>
